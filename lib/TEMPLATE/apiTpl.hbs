import ajax from '@/common/utils/ajax';

{{#each data}}
/* {{name}} */
/* wbneicontrol: on */
export function {{path}}({{#isEqual requestMethods 'get'}}params{{else}}data{{/isEqual}}) {
  return ajax.{{requestMethods}}('/api/{{../className}}/{{path}}', {
    {{#isEqual requestMethods 'get'}}
    params
    {{else}}
    data
    {{/isEqual}}
  });
}

