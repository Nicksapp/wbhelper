{{#if isInit}}
import { Application } from 'egg';

export default (app: Application) => {
  const { controller, router } = app;

  const parseRpc = app.middleware.parseRpc();

  {{#each data}}
  // {{name}}
  router.{{requestMethods}}('/api/{{../className}}/{{path}}', parseRpc, controller.wbnei.{{../className}}.{{path}});
  {{/each}}

}
{{else}}

{{#each data}}
  // {{name}}
  router.{{requestMethods}}('/api/{{../className}}/{{path}}', parseRpc, controller.wbnei.{{../className}}.{{path}});
{{/each}}
{{/if}}